<h1>users</h1>
<table>
	<tr>
		<th>image</th>
		<th>name</th>
		<th></th>
	</tr>
	<% @users.each do |user| %>
		<tr>
			<td>
				<%= attachment_image_tag user, :profile_image,:fill, 100, 100, fallback: "no_image.jpg" %> 
			</td>
			<td>
				<%= user.name %>
			</td>
			<td>
				<%= link_to "show", user_path(user) %>
			</td>
		</tr>
	<% end %>
</table>
